{
    "manifest_version": 2,
    "name": "My Cool Extension",
    "version": "0.1",
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
                // "http://*/*"
            ],
            "js":[
                "jquery.js",
                "content.js"
            ]
        }
    ],
    "background":{
        "scripts":[
            "background.js"
        ]
    },
    "browser_action":{
        "default_icon":{
            "16":"icon.png",
            "24":"icon2.png"
        }
    }
  }